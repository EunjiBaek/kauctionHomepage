<script id="tp_snb_left" type="text/template">
    {{#rows}}
    <div class="view t{{t_num}}" id="t_seq_{{t_seq}}" style="display:block;">
        <ul>
            {{#lotinfos}}
            <li data-lot_num="{{lot_num}}" class="{{^successful_bid}}succ{{/successful_bid}}">
                <div class="works-img lotinfoforselect" data-lot_num="{{lot_num}}" onclick="LiveFunc.fn_selected_lotinfo(event);">
                    <img class="lazy lotinfoforselect" src="/img/live/list_noimg.jpg" data-src="{{img_file_name}}" onerror="this.src='/img/live/list_noimg.jpg'" alt="{{w_name}}" title="{{w_name}}" loading="lazy" data-lot_num="{{lot_num}}">
                </div>
                <div class="works-desc lotinfoforselect" data-lot_num="{{lot_num}}" onclick="LiveFunc.fn_selected_lotinfo(event);">
                    <h6 data-lot_num="{{lot_num}}" class="lotinfoforselect">{{lot_num}}</h6>
                    <p class="tit lotinfoforselect" data-lot_num="{{lot_num}}">{{a_name}}</p>
                    <p class="sub-tit lotinfoforselect" data-lot_num="{{lot_num}}">{{w_name}}</p>
                    <p class="price lotinfoforselect" data-lot_num="{{lot_num}}">
                        {{^successful_bid}}
                        @L["Mid_Left_SuccessBidPrice"] KRW {{#PriceComma}}{{successful_bid_price}}{{/PriceComma}}
                        {{/successful_bid}}
                        {{#successful_bid}}
                        KRW {{#PriceComma}}{{w_low_price}}{{/PriceComma}} ~ {{#PriceComma}}{{w_high_price}}{{/PriceComma}}
                        {{/successful_bid}}
                    </p>
                </div>
                <div class="like" data-work_seq="{{work_seq}}">
                    <a href="#" onclick="LiveFunc.fn_wish_insert();return false;" class="like" data-work_seq="{{work_seq}}">
                        {{^isWish}}
                        <img src="/img/live/invalid-name.png" alt="nomal image" class="like" data-work_seq="{{work_seq}}">
                        {{/isWish}}
                        {{#isWish}}
                        <img src="/img/live/invalid-name2.png" class="like" data-work_seq="{{work_seq}}">
                        {{/isWish}}
                    </a>
                </div>
            </li>
            {{/lotinfos}}
        </ul>
    </div>
    {{/rows}}
</script>

