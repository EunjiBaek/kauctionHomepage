@section Scripts {
    <script>
        var stime = new Date().getTime();
    </script>

    <script src="~/js/signalr/dist/browser/signalr.js"></script>
    <script src="/js/ka.live.@(commonService.IsKor() ? "ko" : "en").js" asp-append-version="true"></script>
    <environment include="Development">
        <script src="/js/ka.live.js?auc_kind=@ViewBag.AucKind&auc_num=@ViewBag.AucNum" asp-append-version="true"></script>
        <script src="/js/ka.live.confirm.js" asp-append-version="true"></script>
        <script src="/js/ka.live.player.js" asp-append-version="true"></script>
        <script src="/js/ka.live.style.js" asp-append-version="true"></script>
    </environment>
    <environment include="Staging,Production">
        <script src="~/bundle/js/ka.live.min.js?auc_kind=@ViewBag.AucKind&auc_num=@ViewBag.AucNum" asp-append-version="true"></script>
    </environment>    
    <script>
        var fn_ws_open = function () {

            Object.defineProperty(WebSocket, 'OPEN', { value: 1, });

            const connection = new signalR.HubConnectionBuilder().withUrl("/liveHub").withAutomaticReconnect().build();

            connection.on("CurrentLotStat", function (message) {
                LiveFunc.fn_curr_lotinfo_complete(JSON.parse(message));

            });


            connection.on("CurrentBidHst", function (message) {
                LiveFunc.fn_curr_lotinfo_bidhst_complete(JSON.parse(message));
            });


            connection.on("ErrorNotification", function (message) {
                toastr.error(message);
            });

            connection.start().then(function () {
                try {
                    connection.invoke("AddToGroup", "@ViewBag.AucNum");
                }
                catch (e) {
                    toastr.error(message);
                }
            }).catch(function (err) {
                toastr.error(err.toString());
                return;
            });

            connection.onreconnected(function (connectionId) {
                console.assert(connection.state === signalR.HubConnectionState.Connected);
                try {
                    connection.invoke("AddToGroup", "@ViewBag.AucNum");
                }
                catch (e) {
                    toastr.error(message);
                }
            });
        };
    </script>
}


